{
    "name": "caseyamcl/php-marc21",
    "type": "library",
    "description": "MARC Record handling tool for PHP",
    "keywords": ["MARC", "conversion", "format", "library"],
    "homepage": "https://github.com/caseyamc/php-marc21",
    "license": "MIT",
    "authors": [
        {
            "name": "Casey McLaughlin",
            "email": "caseyamcl@gmail.com",
            "homepage": "https://caseymclaughlin.com",
            "role": "Developer"
        },
        {
            "name": "Grant Gingell",
            "homepage": "http://www.grantgingell.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {"PhpMarc\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"PhpMarc\\": "tests/"}
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
